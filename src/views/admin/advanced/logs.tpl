<div class="logs">
	<div class="col-lg-9">
		<div class="panel panel-default">
			<div class="panel-heading"><i class="fa fa-file-text-o"></i>[[admin:logs.logs]]</div>
			<div class="panel-body">
				<pre>{data}</pre>
			</div>
		</div>
	</div>
	<div class="col-lg-3 acp-sidebar">
		<div class="panel panel-default">
			<div class="panel-heading">[[admin:logs.logs_control_panel]]</div>
			<div class="panel-body">
				<button class="btn btn-primary" data-action="reload"><i class="fa fa-refresh"></i>[[admin:logs.reload_logs]]</button>
				<button class="btn btn-warning" data-action="clear"><i class="fa fa-eraser"></i>[[admin:logs.clear_logs]]</button>
			</div>
		</div>
	</div>
</div>
