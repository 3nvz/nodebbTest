<div class="logs">
	<div class="col-lg-9">
		<div class="card">
			<div class="card-header"><i class="fa fa-file-text-o"></i> Logs</div>
			<div class="card-block">
				<pre>{data}</pre>
			</div>
		</div>
	</div>
	<div class="col-lg-3 acp-sidebar">
		<div class="card">
			<div class="card-header">Logs Control Panel</div>
			<div class="card-block">
				<button class="btn btn-primary" data-action="reload"><i class="fa fa-refresh"></i> Reload Logs</button>
				<button class="btn btn-warning" data-action="clear"><i class="fa fa-eraser"></i> Clear Logs</button>
			</div>
		</div>
	</div>
</div>
